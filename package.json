{
  "name": "autodeck",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@google-cloud/language": "^3.2.6",
    "axios": "^0.19.0",
    "cookie-parser": "^1.4.4",
    "googleapis": "^39.2.0",
    "history": "^4.9.0",
    "morgan": "^1.9.1",
    "react": "^16.9.0",
    "react-audio-recorder": "^2.2.0",
    "react-dom": "^16.9.0",
    "react-mic": "^12.4.1",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1",
    "react-speech-recognition": "^2.0.4",
    "socket.io": "^2.2.0"
  },
  "scripts": {
    "start-react": "react-scripts start",
    "start": "node api/app.js",
    "build": "react-scripts build && cp -r build/* api/public",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --ignore-path .gitignore ."
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "indent": [
        "error",
        2
      ],
      "quotes": [
        2,
        "single"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint": "^6.3.0",
    "eslint-plugin-react": "^7.14.3"
  }
}
